    "@types/inquirer": "^6.5.0",
    "@types/node": "^12.7.1",
    "@types/rimraf": "^2.0.2",
    "cross-env": "^5.2.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
