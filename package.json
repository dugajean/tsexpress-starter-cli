{
  "name": "@tsexpress-starter/cli",
  "version": "0.1.0",
  "description": "CLI tool to quickly setup TSExpress Starter.",
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "keywords": [
    "create-app",
    "tsexpress-starter",
    "cli"
  ],
  "author": "Dukagjin Surdulli <me@dukagj.in> (https://dukagj.in/)",
  "license": "MIT",
  "bin": {
    "@tsexpress-starter/cli": "dist/cli",
